    "babel-register": "^6.9.0",
    "cross-env": "^5.2.0",
    "match-index": "^1.0.1",
    "mocha": "^5.2.0",
    "power-assert": "^1.6.1",
    "rimraf": "^2.6.2",
    "textlint": "^11.0.2",
