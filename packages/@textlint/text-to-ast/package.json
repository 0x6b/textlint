{
  "name": "@textlint/text-to-ast",
  "description": "Parse plain text to AST with location info.",
  "version": "3.0.3",
  "author": "azu",
  "bugs": {
    "url": "https://github.com/textlint/textlint/issues"
  },
  "dependencies": {
    "@textlint/ast-node-types": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-jsdoc-to-assert": "^4.0.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.24.1",
    "cross-env": "^4.0.0",
    "mocha": "^4.0.1",
    "power-assert": "^1.4.2",
    "rimraf": "^2.6.2",
    "textlint-ast-test": "^2.0.2"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "homepage": "https://github.com/textlint/textlint/tree/master/packages/@textlint/text-to-ast/",
  "keywords": [
    "ast",
    "textlint",
    "txt"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/textlint/textlint.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib --source-maps",
    "clean": "rimraf lib/",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.{js,ts}\"",
    "watch": "babel src --out-dir lib --watch --source-maps"
  }
}
