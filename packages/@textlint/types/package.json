{
  "name": "@textlint/types",
  "version": "1.0.0",
  "description": "Type definition pacakge for textlint",
  "keywords": [
    "definition",
    "textlint",
    "types",
    "typescript"
  ],
  "homepage": "https://github.com/textlint/textlint/tree/master/packages/@textlint/types/",
  "bugs": {
    "url": "https://github.com/textlint/textlint/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textlint/textlint.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/"
  ],
  "main": "lib/types/src/index.js",
  "types": "lib/types/src/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "watch": "tsc -p . --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "dependencies": {
    "structured-source": "^3.0.2",
    "@textlint/ast-node-types": "^4.0.3"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/structured-source": "^3.0.0",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "ts-node-test-register": "^4.0.0",
    "typescript": "^3.2.2",
    "unist-util-select": "^2.0.0",
    "@textlint/markdown-to-ast": "^6.0.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
